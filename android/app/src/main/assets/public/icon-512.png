<!-- Icône 512x512 - Version SVG convertie pour Google Play -->
<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg512" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8b5cf6"/>
      <stop offset="30%" style="stop-color:#ec4899"/>
      <stop offset="70%" style="stop-color:#3b82f6"/>
      <stop offset="100%" style="stop-color:#8b5cf6"/>
    </linearGradient>
    <filter id="shadow">
      <feDropShadow dx="0" dy="4" stdDeviation="8" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" rx="112" fill="url(#bg512)"/>
  
  <!-- Pattern overlay -->
  <rect width="512" height="512" rx="112" fill="url(#bg512)" opacity="0.1"/>
  
  <!-- Main heart icon -->
  <g transform="translate(256,200)">
    <path d="M-60,-40 C-80,-40 -100,-20 -100,10 C-100,40 -60,80 0,120 C60,80 100,40 100,10 C100,-20 80,-40 60,-40 C40,-40 20,-20 0,-10 C-20,-20 -40,-40 -60,-40 Z" 
          fill="white" filter="url(#shadow)"/>
    <circle cx="0" cy="10" r="20" fill="#8b5cf6"/>
  </g>
  
  <!-- App name -->
  <text x="256" y="380" text-anchor="middle" fill="white" font-size="48" font-weight="bold" font-family="Arial, sans-serif">
    Co-Parents
  </text>
  
  <!-- Tagline -->
  <text x="256" y="420" text-anchor="middle" fill="white" font-size="18" font-weight="normal" font-family="Arial, sans-serif" opacity="0.9">
    Familles recomposées
  </text>
</svg>