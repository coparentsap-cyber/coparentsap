{
	"appId": "com.coparents.app",
	"appName": "Co-Parents",
	"webDir": "dist",
	"server": {
		"androidScheme": "https"
	},
	"android": {
		"allowMixedContent": true,
		"captureInput": true,
		"webContentsDebuggingEnabled": false
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 2000,
			"backgroundColor": "#8b5cf6",
			"showSpinner": false,
			"androidSpinnerStyle": "large",
			"spinnerColor": "#ffffff"
		},
		"StatusBar": {
			"style": "default",
			"backgroundColor": "#8b5cf6"
		}
	}
}
